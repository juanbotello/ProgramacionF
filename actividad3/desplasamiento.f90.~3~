 ! outputdata.f90

  implicit none
  real, dimension(100) :: x, y
  real, (a - b, o - z)
  integer :: i, j, t, l, m, n
   

  ! setup x and y with some data
  do i = 1, 100
     x = v0 * t * cos(a)
     y = v0 * t * sin(a) - 1/2 * (g * t) * (g * t)
     end do 
  ! output data to a file
  open(1, file='data1.dat', status='unknown')
  do i=1,100
     write(1,*) x(i), y(i)
      
  end do
  close(1)

end program outputdata

