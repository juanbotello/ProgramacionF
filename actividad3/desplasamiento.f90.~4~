 ! outputdata.f90

  implicit none
  real, g ( 9.8) :: x, y, 
  real, (a - b, o - z)
  integer :: i, j, t, l, m, n
  real, pi 3.141592
  

  ! setup x and y with some data
  do i = 1, 100
     
     vx = v0 * t * cos(radianes)

     vy = v0 * t * sin(radianes) - 1/2 * (g * t) * (g * t)

  end do
  ! output data to a file
  open(1, file='data1.dat', status='unknown')
  do i=1,100
     write(1,*) x(i), y(i)
      
  end do
  close(1)

end program outputdata

